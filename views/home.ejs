<link rel="stylesheet" href="css/home.css">
    <%- include('_header');%>
<div class="item">
    <ul class="d-flex ">
        <%for(let i of list){%>
            <li class="li">
                <div class="card">
                    <img src="https://tse1.mm.bing.net/th?id=OIP.nFtlTeqHyG2yQqh5olfSLgHaEK&pid=Api&P=0&h=180"
                        class="card-img-top" alt="...">
                    <div class="card-body">
                        <h5 class="card-title"> Name:- <%=i.name%>
                        </h5>
                        <h5 class="card-title card-text">Description:- <%=i.desc%></h5>
                        <h5 class="card-title">
                            Author Name:- <%=i.auth%>
                        </h5>
                        <a href="/user" class="btn btn-primary">Open Project</a>
                       <a href="/delete/?id=<%=i._id%>"><input type="submit" class="btn btn-danger" value="Delete"></a> 
                    </div>
                </div>
            </li>
            <%}%>
        </ul>
            <!-- <img src="https://blog.ezofficeinventory.com/wp-content/uploads/2019/06/Issue-tracking-software-1.jpg" alt="" width="1400"> -->
</div>